add wave -position end  sim:/Simulation/processing/ENTRADA_DADO
add wave -position end -radix decimal  sim:/Simulation/processing/SAIDA_DADO
add wave -position end -radix decimal  sim:/Simulation/processing/A
add wave -position end -radix decimal  sim:/Simulation/processing/B
add wave -position end  sim:/Simulation/processing/SAIDA
add wave -position end  sim:/Simulation/processing/SELETOR

run 2 ns

