vlog -f Modulos_SV
vcom -f Modulos_VHD

vsim -L altera_mf_ver -gui work.Simulation

add wave -position end -radix decimal  sim:/Simulation/processing/CLK
add wave -position end  sim:/Simulation/processing/RST
